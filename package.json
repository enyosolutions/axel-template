    "@types/async": "^3.0.8",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.30",
    "@types/chai": "^4.2.4",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/express-request-id": "^1.4.1",
    "@types/faker": "^4.1.11",
    "@types/helmet": "0.0.45",
