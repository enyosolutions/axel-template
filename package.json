{
  "name": "{{name}}",
  "version": "0.0.1",
  "description": "Express + typescript in the Sailsjs flavor",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && npx tsc",
    "build": "cross-env TS_NODE_FILES=true ts-node build.ts && npx tsc",
    "dev": "cross-env TS_NODE_FILES=true nodemon src/index.ts | pino-pretty",
    "dev:debug": "TS_NODE_FILES=true nodemon --exec 'node --inspect -r ts-node/register ./src/index.ts | pino-pretty'",
    "lint": "./node_modules/.bin/tsc --noEmit && eslint -c .eslintrc.js '{src, test}/*.{js,ts,tsx}' --quiet",
    "lint:fix": "tsc --noEmit && eslint -c .eslintrc.js '{src, test}/*.{js,ts,tsx}' --quiet --fix",
    "test": "TS_NODE_FILES=true jest",
    "testp": "TS_NODE_FILES=true mocha-parallel-tests  -r ts-node/register test/bootstrap.test.ts test/**/*.ts --exit --bail",
    "test:debug": "mocha --inspect-brk test/bootstrap.test.ts  test/**/*.ts --exit",
    "data:load": "AXEL_DISABLE_EMAILS=true TS_NODE_FILES=true NO_SEQUELIZE_LOGS=true  ts-node scripts/loadData.ts"
  },
  "dependencies": {
    "ajv": "^6.12.2",
    "async": "^3.2.0",
    "axel-cli": "^0.0.9",
    "axios": "^0.18.0",
    "bcrypt": "^4.0.1",
    "better-console": "^1.0.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "express-openapi-validator": "^3.6.6",
    "express-request-id": "^1.4.1",
    "flat": "^4.1.0",
    "googleapis": "^61.0.0",
    "helmet": "^3.21.2",
    "js-yaml": "^3.13.1",
    "json-stringify-safe": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "minimist": "^1.2.5",
    "moment": "^2.24.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.0",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.4.6",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "pino": "^6.2.1",
    "sequelize": "^5.21.5",
    "skipper": "^0.8.7",
    "socket.io": "^2.3.0",
    "swagger-jsdoc": "^4.0.0",
    "xlsx": "^0.14.0"
  },
