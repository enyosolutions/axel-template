    "supertest": "^3.0.0",
    "ts-jest": "^26.1.0",
    "ts-lint": "^4.5.1",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.1",
    "typescript": "^3.9.2"
  },
  "lint-staged": {
    "package.json": [
      "prettier-package-json --write"
    ],
    "front/package.json": [
      "prettier-package-json --write"
    ],
    "front/src/config/locales/*.json": [
      "jsonlint -i -s"
    ],
    "*.js": [
      "yarn lint:fix",
      "git add"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "front/*",
      "docs/*",
      "test/*",
      "tests/*"
    ],
    "delay": "750"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:fix"
    }
  },
  "author": "Edouard BATIGA <dev@enyosolutions.com> (https://github.com/enyosolutions"
}
